# DBFS path to the HTML file
html_file_path = "dbfs:/path/to/your/html/file.html"

# Function to read and display HTML content
def display_html_from_dbfs(file_path):
    with open(file_path, "r") as html_file:
        html_content = html_file.read()
        displayHTML(html_content)

# Display the HTML content from the specified DBFS path
display_html_from_dbfs(html_file_path)
