\COPY (
    SELECT 
        '=' || '"' || phone_number || '"' AS phone_number,
        -- Add other columns as needed, or just use *
        *
    FROM public.your_table_name
    WHERE updatetime BETWEEN '2025-05-19 00:00:01.253' AND '2025-05-20 00:00:01.253'
) TO '/your/local/path/output.txt'
WITH DELIMITER E'\t' NULL '' CSV HEADER ENCODING 'UTF8';



