SELECT 
    'SELECT ''' || table_name || ''' AS table_name, COUNT(*) AS row_count, ' ||
    'CASE WHEN COUNT(*) > 0 THEN ''LOADED'' ELSE ''NOT LOADED'' END AS status ' ||
    'FROM ' || table_name || 
    ' UNION ALL'
AS generated_sql
FROM user_tables
WHERE table_name IN ('TABLE1', 'TABLE2', 'TABLE3');  -- Add your table names here
