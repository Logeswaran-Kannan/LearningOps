DO
$$
DECLARE
    r RECORD;
BEGIN
    FOR r IN
        SELECT tablename
        FROM pg_tables
        WHERE schemaname = 'public'  -- Change schema if needed
    LOOP
        EXECUTE format('SELECT count(*) FROM public.%I', r.tablename) INTO r;
        RAISE NOTICE 'Table: %, Count: %', r.tablename, r.count;
    END LOOP;
END;
$$;
