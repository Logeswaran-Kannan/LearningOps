SELECT
    enduedate,
    get_json_object(interactionmessage, '$._id') AS interaction_id,
    (LENGTH(payload) / (1024.0 * 1024.0)) AS payload_size_mb,
    payload
FROM core_rl_test.flow.ehubstream
WHERE get_json_object(interactionmessage, '$._id') = '12RG45TY'
ORDER BY payload_size_mb DESC
LIMIT 5;
